{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "db",
    "public": false
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false,
    "relations": {
      "user": {
        "type": "belongsTo",
        "model": "AppUser",
        "foreignKey": "userId"
      }
    }
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "db",
    "public": false,
    "relations": {
      "users": {
        "type": "hasMany",
        "model": "AppUser",
        "foreignKey": "roleId",
        "through": "RoleMapping"
      }
    }
  },
  "Note": {
    "dataSource": "db"
  },
  "AppUser": {
    "dataSource": "db",
    "public": true,
    "relations": {
      "accessTokens": {
        "type": "hasMany",
        "model": "AccessToken",
        "foreignKey": "userId",
        "options": {
          "disableInclude": true
        }
      },
      "roles": {
        "type": "hasMany",
        "model": "Role",
        "foreignKey": "principalId",
        "through": "RoleMapping"
      }
    }
  }
}
